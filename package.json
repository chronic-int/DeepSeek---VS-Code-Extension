{
    "name": "deepseek-vscode-agent",
    "publisher": "chronic-int",
    "displayName": "DeepSeek VS Code Agent",
    "description": "Autonomous Workspace AI Assistant powered by DeepSeek",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "AI",
        "Programming Languages",
        "Education"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "deepseek-agent-sidebar",
                    "title": "DeepSeek Agent",
                    "icon": "media/icon.svg"
                }
            ]
        },
        "views": {
            "deepseek-agent-sidebar": [
                {
                    "type": "webview",
                    "id": "deepseek-agent-chat",
                    "name": "Chat"
                }
            ]
        },
        "authentication": [
            {
                "label": "DeepSeek",
                "id": "deepseek"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.11.0",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "@vscode/test-electron": "^2.3.9",
        "eslint": "^8.56.0",
        "glob": "^10.3.10",
        "mocha": "^10.2.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@types/uuid": "^10.0.0",
        "axios": "^1.7.9",
        "uuid": "^13.0.0"
    }
}